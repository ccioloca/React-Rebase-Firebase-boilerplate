{
  "name": "reactfirebaserebaseboilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "firebase-admin": "^4.0.4",
    "node-sass": "^3.13.0",
    "npm-scripts-watcher": "^1.0.2",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "bonescss": "^0.8.4",
    "classnames": "^2.2.5",
    "dialog-size": "^1.2.1",
    "firebase-queue": "^1.6.1",
    "jsxstyle": "^1.0.1",
    "moment": "^2.17.0",
    "re-base": "^2.2.0",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.4.0",
    "react-addons-pure-render-mixin": "^15.4.0",
    "react-bootstrap": "^0.30.6",
    "react-breadcrumbs": "^1.5.0",
    "react-dd-menu": "^1.0.7",
    "react-dom": "^15.3.2",
    "react-loading": "^0.0.9",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "sensible": "^0.8.2",
    "uuid": "^3.0.1",
    "validator": "^6.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "magic": ". ./magic.sh",
    "compile-sass": "node-sass src/index.scss -o src --include-path node_modules",
    "watch": "npm-scripts-watcher",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "server": "nodemon queue.js --exec babel-node --presets es2015,stage-2",
    "q": "nodemon addtoqueue.js --exec babel-node --presets es2015,stage-2"
  },
  "watch": {
    "{src,test}/**/*.scss": [
      "compile-sass"
    ]
  }
}
