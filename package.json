{
  "name": "reactfirebaserebaseboilerplate",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "npm-scripts-watcher": "^1.0.2",
    "react-scripts": "0.6.1"
  },
  "dependencies": {
    "bonescss": "^0.8.4",
    "classnames": "^2.2.5",
    "jsxstyle": "^1.0.1",
    "re-base": "^2.2.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.6",
    "react-breadcrumbs": "^1.5.0",
    "react-dom": "^15.3.2",
    "react-loading": "^0.0.9",
    "react-router": "^3.0.0",
    "react-router-bootstrap": "^0.23.1",
    "sensible": "^0.8.2",
    "validator": "^6.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "compile-sass": "node-sass src/index.scss -o src --include-path node_modules",
    "watch": "npm-scripts-watcher",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "watch": {
    "{src,test}/**/*.scss": [
      "compile-sass"
    ]
  }
}
